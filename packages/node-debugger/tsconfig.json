{
  "compilerOptions": {
    "target": "es2020",
    "resolveJsonModule": true,
    "removeComments": true,
    "allowJs": true,
    "module": "es2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "strictBindCallApply": false,
    "skipLibCheck": true

  }
}
